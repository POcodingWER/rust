fn main() {
    let mut v = vec![1, 2, 3, 4, 5];
    let first = &v[0];
    println!("{:?}", first);
    v.push(6);
    println!("{:?}", v);

    let v = vec![100, 32, 57];
    for i in &v {
        println!("{}", i);
    }
    
    let mut v = vec![100, 32, 57];
    for i in &mut v {
        *i += 50;
    }
    println!("{:?}", v);
}
